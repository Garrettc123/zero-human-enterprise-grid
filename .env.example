# ==================== CLOUD PROVIDERS ====================

# AWS
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key

# Google Cloud
GCP_PROJECT_ID=your_project_id
GCP_CREDENTIALS_PATH=/path/to/credentials.json

# Azure
AZURE_SUBSCRIPTION_ID=your_subscription_id
AZURE_TENANT_ID=your_tenant_id
AZURE_CLIENT_ID=your_client_id
AZURE_CLIENT_SECRET=your_client_secret

# Render
RENDER_API_KEY=your_api_key

# Vercel
VERCEL_TOKEN=your_token

# ==================== DATABASES ====================

# PostgreSQL Production
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=production
POSTGRES_USER=postgres
POSTGRES_PASSWORD=secure_password

# PostgreSQL Backup
POSTGRES_BACKUP_HOST=localhost
POSTGRES_BACKUP_PORT=5433
POSTGRES_BACKUP_DB=backup

# MongoDB
MONGODB_URI=mongodb://localhost:27017
MONGODB_DATABASE=analytics

# DynamoDB
DYNAMODB_REGION=us-east-1

# Elasticsearch
ELASTICSEARCH_HOST=localhost
ELASTICSEARCH_PORT=9200
ELASTICSEARCH_SCHEME=http

# ==================== STORAGE ====================

# AWS S3
S3_BUCKET=main-bucket
S3_REGION=us-east-1

# Google Cloud Storage
GCS_BUCKET=main-bucket

# Azure Blob
AZURE_STORAGE_ACCOUNT=storageaccount
AZURE_STORAGE_CONTAINER=main-container
AZURE_STORAGE_KEY=your_key

# MinIO
MINIO_HOST=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_SECURE=false

# ==================== CACHE ====================

# Redis Primary
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# Redis Replica
REDIS_REPLICA_HOST=localhost
REDIS_REPLICA_PORT=6380
REDIS_REPLICA_DB=0

# ==================== MESSAGE QUEUES ====================

# Kafka
KAFKA_BROKERS=localhost:9092
KAFKA_SECURITY_PROTOCOL=PLAINTEXT

# RabbitMQ
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=admin
RABBITMQ_PASSWORD=admin
RABBITMQ_VHOST=/

# AWS SQS
SQS_REGION=us-east-1
SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/your-account-id/your-queue

# ==================== SEARCH ENGINES ====================

# Elasticsearch
ELASTICSEARCH_HOST=localhost
ELASTICSEARCH_PORT=9200
ELASTICSEARCH_USERNAME=
ELASTICSEARCH_PASSWORD=

# Algolia
ALGOLIA_APP_ID=your_app_id
ALGOLIA_API_KEY=your_api_key

# Meilisearch
MEILISEARCH_URL=http://localhost:7700
MEILISEARCH_API_KEY=your_api_key

# ==================== ML PLATFORMS ====================

# MLflow
MLFLOW_TRACKING_URI=http://localhost:5000
MLFLOW_BACKEND_STORE_URI=postgresql://postgres@localhost:5432/mlflow
MLFLOW_DEFAULT_ARTIFACT_ROOT=/mlflow/artifacts

# AWS SageMaker
SAGEMAKER_REGION=us-east-1
SAGEMAKER_ROLE_ARN=arn:aws:iam::your-account-id:role/SageMakerRole

# Google Vertex AI
VERTEX_AI_PROJECT_ID=your_project_id
VERTEX_AI_LOCATION=us-central1

# Weights & Biases
WANDB_API_KEY=your_api_key
WANDB_ENTITY=your_entity
WANDB_PROJECT=your_project

# ==================== GRAPHQL ====================

GRAPHQL_ENDPOINT_1=https://graphql.prod.example.com/graphql
GRAPHQL_TOKEN_1=your_token_1

GRAPHQL_ENDPOINT_2=https://graphql.backup.example.com/graphql
GRAPHQL_TOKEN_2=your_token_2

# ==================== MONITORING ====================

PROMETHEUS_URL=http://localhost:9090
GRAFANA_URL=http://localhost:3000
GRAFANA_API_KEY=your_api_key

# CloudWatch
CLOUDWATCH_LOG_GROUP=/aws/orchestrator
CLOUDWATCH_REGION=us-east-1

# ==================== APPLICATION ====================

# API
API_HOST=0.0.0.0
API_PORT=8000
API_DEBUG=false

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Performance
SYNC_INTERVAL_CLOUD=60
SYNC_INTERVAL_DATABASE=30
SYNC_INTERVAL_STORAGE=30
SYNC_INTERVAL_CACHE=20
SYNC_INTERVAL_MESSAGES=15
SYNC_INTERVAL_SEARCH=25
SYNC_INTERVAL_ML=45
SYNC_INTERVAL_GRAPHQL=35
MONITORING_INTERVAL=10

# Batch Processing
BATCH_SIZE_DATABASE=1000
BATCH_SIZE_STORAGE=100
BATCH_SIZE_SEARCH=500
BATCH_SIZE_MESSAGES=100

# Timeouts (seconds)
TIMEOUT_DATABASE=30
TIMEOUT_STORAGE=60
TIMEOUT_API=10
TIMEOUT_GRAPHQL=15
